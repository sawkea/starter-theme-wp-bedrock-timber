{% block html_head_container %}

{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}

	<body class="{{body_class}}" data-template="base.twig">
		<header class="header" >
			{% block header %}
					<nav id="nav-main" class="nav-main" role="navigation">
						{% include "menu.twig" with {'items': menu.get_items} %}
					</nav><!-- #nav -->
				</div>
			{% endblock %}
		</header>
		{% block content %}
			
		{% endblock content %}

		{% block footer %}
			<footer id="footer">
				<div>{% include 'footer.twig' %}</div>
			</footer>
			{{ function('wp_footer') }}
		{% endblock %}
	</body>
</html>
